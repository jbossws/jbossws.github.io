<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">

  <enterprise-beans>
    <session>

      <ejb-name>EJB3Bean</ejb-name>
      <ejb-class>org.jboss.test.ws.jaxws.jbws2634.webservice.EJB3Bean</ejb-class>

      <env-entry>
        <env-entry-name>boolean1</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>

    </session>
    <session>

      <ejb-name>EJB3Bean2</ejb-name>
      <ejb-class>org.jboss.test.ws.jaxws.jbws2634.webservice.EJB3Bean2</ejb-class>

      <env-entry>
        <env-entry-name>boolean1</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>

    </session>
    <session>

      <ejb-name>EJB3Bean3</ejb-name>
      <ejb-class>org.jboss.test.ws.jaxws.jbws2634.webservice.EJB3Bean3</ejb-class>

      <env-entry>
        <env-entry-name>boolean1</env-entry-name>
        <env-entry-type>java.lang.Boolean</env-entry-type>
        <env-entry-value>true</env-entry-value>
      </env-entry>

    </session>

  </enterprise-beans>

</ejb-jar>
